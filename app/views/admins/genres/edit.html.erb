<div class="container">
   <div class="row my-3">
    <h2>ジャンル編集</h2>
   </div>
  <div class="row">

      <%= form_with model: @genre, url: admins_genre_path, method: :put, local: true do |f| %>
      <table>
        <th>ジャンル名</th>
        <th><%= f.text_field :name %></th>
        <th><%= f.submit '変更を保存' , class: 'btn btn-success' %></th>
      </table>
      <% end %>
  </div>
</div>